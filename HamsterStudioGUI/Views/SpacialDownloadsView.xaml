<UserControl x:Class="HamsterStudioGUI.Views.SpacialDownloadsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:HamsterStudioGUI.Views"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:spdownloads="clr-namespace:HamsterStudioGUI.Views.SpacialDownloads"
             d:DesignHeight="450"
             d:DesignWidth="800"
             mc:Ignorable="d">
    <TabControl TabStripPlacement="Top">
        <TabItem Header="直链下载">
            <spdownloads:DirectLinkDownload DataContext="{Binding DirectLinkDownload}" />
        </TabItem>
        <TabItem Header="Bilibili">
            <spdownloads:BilibiliSpDownloads DataContext="{Binding BilibiliSpDownloadsViewModel}" />
        </TabItem>
    </TabControl>
</UserControl>
